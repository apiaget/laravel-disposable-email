{
    "name": "erag/laravel-disposable-email",
    "description": "A Laravel package to detect and block disposable email addresses.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "disposable",
        "email",
        "validation",
        "email validator",
        "block disposable",
        "disposable check",
        "laravel email rule",
        "email blacklist",
        "temporary email",
        "fake email",
        "email validation rule",
        "laravel custom rule",
        "email filtering",
        "email domain checker"
    ],
    "authors": [
        {
            "name": "Er Amiit Gupta",
            "email": "info.eramitgupta@gmail.com",
            "homepage": "https://github.com/eramitgupta"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "laravel/pint": "^1.13",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/validation": "^10.0|^11.0|^12.0",
        "illuminate/translation": "^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "EragLaravelDisposableEmail\\": "src/"
        }
    },
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "EragLaravelDisposableEmail\\LaravelDisposableEmailServiceProvider"
            ],
            "aliases": {
                "DisposableEmail": "EragLaravelDisposableEmail\\Facades\\DisposableEmail"
            }
        }
    }
}
